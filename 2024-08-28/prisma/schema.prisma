generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id         Int      @id @default(autoincrement())
  status     String   
  userName   String   @unique
  birthDate  DateTime
  city       String
  region     String
  phone      String?
  avatar     String?
  bio        String?
  createdAt  DateTime @default(now())
}
